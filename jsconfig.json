        "paths": {
            "@constants/*": [
                "./src/utils/constants/*"
            ],
            "@helpers/*": [
                "./src/utils/helpers/*"
            ],
            "@services/*": [
                "./src/utils/services/*"
            ],
            "@utils/*": [
                "./src/utils/*"
            ],
            "@hoc/*": [
